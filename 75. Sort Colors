class Solution {
    public void sortColors(int[] nums) {
        /* BRUTE FORCE
          Arrays.sort(nums);  -> O(N logN)
        */
      
        //swapping -> tc -> O(N^2)
        
        int n = nums.length;
        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                if (nums[i] > nums[j]) {
                    // Swap nums[i] and nums[j]
                    int temp = nums[i];
                    nums[i] = nums[j];
                    nums[j] = temp;
                }
            }
        }
    }
}
