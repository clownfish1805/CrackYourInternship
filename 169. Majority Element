class Solution {
    public int majorityElement(int[] nums) {
        
        int n = nums.length;
        HashMap<Integer, Integer> res = new HashMap<>();
        for (int i = 0; i < n; i++) {
            res.put(nums[i],res.getOrDefault(nums[i],0)+1);
        }
        for (Map.Entry<Integer, Integer> i : res.entrySet()) {
            if (i.getValue() > (n / 2)) {
                return i.getKey();
            }
        }
        return -1;
        
    }
}
