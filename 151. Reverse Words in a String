class Solution {
    public String reverseWords(String s) {
       /*
       s.trim();
       String[] str=s.split("\\s+");
       String ans="";
       for(int i=str.length-1; i>=0;i--){
        ans+=str[i]+" ";
       }
       return ans.trim();
       
       */

       String[] str=s.trim().split("\\s+");
       StringBuilder ans=new StringBuilder();

       for(int i=str.length-1;i>=0;i--){
        ans.append(str[i]);
        if(i!=0){
            ans.append(" ");
        }
       }

       return ans.toString();
    }
}
