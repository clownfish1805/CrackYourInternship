class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        /*
        BRUTE FORCE -> O(M+N)+O(log M+N)
        
        for(int i=0;i<n;i++){
            nums1[m+i]=nums2[i];
        }
        Arrays.sort(nums1);
        */
        
        //OPTIMAL SOL -> O(M+N)
        
        int[] res = new int[m + n];
        int i = 0, j = 0, k = 0;
        
  
        while(i<m && j<n) {      //merging arrays
            if (nums1[i] <= nums2[j]) {
                res[k++] = nums1[i++];
            } else {
                res[k++] = nums2[j++];
            }
        }
      
        while(i<m) {            //copying rem elements from 1st array
            res[k++] = nums1[i++];
        }
        
        
        while(j<n) {            //copy rem elements from 2nd array
            res[k++] = nums2[j++];
        }
      
        for(int ind=0;ind<m+n;ind++) {           //copy back res array to 1st array
            nums1[ind] = res[ind];
        }
    }
}
