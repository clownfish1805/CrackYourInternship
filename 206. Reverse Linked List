class Solution {
    public ListNode reverseList(ListNode head) {
        //TC-> O(N) -> ITERATIVE APPROACH

       ListNode temp=head;
       ListNode prev=null;
       while(temp!=null){
        ListNode front=temp.next;
        temp.next=prev;
        prev=temp;
        temp=front;
       }
       return prev;
    }
}
